<view class='viewImag'>
  <swiper indicator-dots='true' autoplay='true' interval='3000'>
    <swiper-item>
      <image src='/images/avatar/swiper3.jpg' data-postid="0" class='border'></image>
    </swiper-item>
    <swiper-item>
      <image src='/images/avatar/swiper2.jpg' class='border'></image>
    </swiper-item>
    <swiper-item>
      <image src='/images/avatar/swiper1.jpeg' class='border'></image>
    </swiper-item>
  </swiper>
</view>

<view class='clickBox'>
  <image src='/images/icon/geren.png' class='Imageicon geren'></image>
  <text class='gerenBook' bindtap='gerenBook'>个人预约</text>
  <view class='line'></view>
  <image src='/images/icon/wode.png' class='Imageicon wode'></image>
  <text class='wodeBook' bindtap='wodeBook'>我的预约</text>
</view>

<view class='clickBox_two'>
  <image src='/images/icon/location.png' class='Imageicon geren'></image>
  <text class='gerenBook' bindtap='navigate'>地图导览</text>
  <view class='line'></view>
  <image src='/images/icon/help.png' class='Imageicon wode'></image>
  <text class='wodeBook' bindtap='Tohelp' >使用帮助</text>
  <view class='line_2'></view>  
  <image src='/images/icon/phone.png' class='Imageicon wode Image_advide'></image>
  <text class='wodeBook advice' bindtap='advice'>意见反馈</text>
</view>

